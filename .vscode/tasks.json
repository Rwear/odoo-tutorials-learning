{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Run Odoo (venv python, parent paths)",
      "type": "shell",
      "command": "powershell",
      "args": [
        "-NoProfile",
        "-ExecutionPolicy", "Bypass",
        "-NoExit",
        "-Command",
        "\"../venv/Scripts/python.exe\" \"../odoo/odoo-bin\" -c \"../odoo.conf\" -u estate --log-level=debug"
      ],
      "options": {
        "cwd": "${workspaceFolder}",
        "env": {
          "PYTHONUTF8": "1"
        }
      },
      "presentation": {
        "reveal": "always",
        "panel": "dedicated",
        "focus": true,
        "clear": true
      },
      "problemMatcher": []
    }
  ]
}
